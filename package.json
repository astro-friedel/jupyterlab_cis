{
  "name": "@jupyterlab/cis-extension",
  "version": "0.0.1",
  "private": true,
  "description": "Crops in Silico Model Composer JupyterLab Extension",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/jupyterlab/jupyterlab_cis.git",
  "bugs": {
    "url": "https://github.com/jupyterlab/jupyterlab_cis.git/issues"
  },
  "license": "BSD-3-Clause",
  "author": "Mike Lambert <lambert8@illinois.edu>",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/jupyterlab/jupyterlab_cis.git"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib",
    "prepack": "npm run clean && npm run build",
    "prettier": "prettier --write '{README,src/*,style/*}{.ts,.tsx,.js,.jsx,.css,.json,.md}'",
    "watch": "tsc -w"
  },
  "dependencies": {
    "@jupyterlab/application": "^0.18.4",
    "@jupyterlab/apputils": "^0.18.4",
    "@phosphor/coreutils": "^1.3.0",
    "@phosphor/messaging": "^1.2.2",
    "@phosphor/widgets": "^1.6.0",
    "font-awesome": "^4.7.0",
    "the-graph": "^0.12.0"
  },
  "devDependencies": {
    "rimraf": "^2.6.1",
    "typescript": "~2.9.2"
  },
  "jupyterlab": {
    "extension": true
  }
}
